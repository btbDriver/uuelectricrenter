<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.youyou.uuelectric.renter.UI.main.SettingActivity">

    <include
        android:id="@+id/fapiao"
        layout="@layout/set_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/s5"
        android:layout_marginTop="@dimen/s4" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/s4"
        android:background="@color/c11"
        android:orientation="vertical"
        >
        <include
            android:id="@+id/paymentNotice"
            layout="@layout/set_item"
            android:layout_width="match_parent"
            android:layout_height="@dimen/s5" />

        <include
            layout="@layout/horizontal_divider"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/s3" />

        <include
            android:id="@+id/peccancyList"
            layout="@layout/set_item" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/s4"
        android:background="@color/c11"
        android:orientation="vertical">

        <include
            android:id="@+id/helper"
            layout="@layout/set_item"
            android:layout_width="match_parent"
            android:layout_height="@dimen/s5" />

        <include
            layout="@layout/horizontal_divider"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/s3" />

        <include
            android:id="@+id/about"
            layout="@layout/set_item" />

        <include
            layout="@layout/horizontal_divider"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/s3" />

        <include
            android:id="@+id/feedback"
            layout="@layout/set_item" />
    </LinearLayout>
</LinearLayout>
