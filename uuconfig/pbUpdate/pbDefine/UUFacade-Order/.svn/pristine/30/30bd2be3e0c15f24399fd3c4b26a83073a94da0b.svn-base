	message Response{
		required int32 ret = 1; //0:成功   -1：失败 1：订单状态已变更（订单状态不是用车中，只有用车中才可还车）2:弹窗提示还车网点及费用
		optional string parkingId = 2;	//返回给客户端的网点ID
		optional string windowMsg = 3;  //弹窗信息（高亮部分）
	}	
}

