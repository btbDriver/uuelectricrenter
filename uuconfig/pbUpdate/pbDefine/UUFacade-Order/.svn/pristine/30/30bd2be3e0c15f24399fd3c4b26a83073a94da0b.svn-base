//支付确认，订单状态更改为支付中
message PayActionCancel{
	message Request{
		repeated string orderId = 1;   //订单号   
		required PayActionConfirmType type = 2;//类型
	}
	message Response{
		required int32 ret = 1;            // 0:成功   -1：失败
	}
}



